{
    "name": "spatie/guzzle-redirect-history-middleware",
    "description": "A Guzzle middleware to keep track of redirects",
    "keywords": [
        "spatie",
        "guzzle-redirect-history-middleware"
    ],
    "homepage": "https://github.com/spatie/guzzle-redirect-history-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\GuzzleRedirectHistoryMiddleware\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\GuzzleRedirectHistoryMiddleware\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
